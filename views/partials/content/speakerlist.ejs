<article class="speakerslist">
	<section class="layout">
		<div class="speakers">
			<% artist.forEach(function(item){ %>
				<article class="artist group">
					<h3><%= item.title %>
						<span class="small"> with 
					<a href="/speakers/<%= item.shortname %>"><%= item.name %></a>
					</span>
				</h3>
					<p><a href="/speakers/<%= item.shortname %>"><img class="speaker"
						src="../images/speakers/<%= item.img %>" alt=""></a>
<% if(page != 'home'){ %>
						<% if(page === 'artistList'){ %>
							<%- item.summary %>
					<% } else { %>
						<%- item.description %>
				<% } %>
					<% } %>



					</p>
						
				</article>
				<% }); %>
		</div>
	</section>
</article>